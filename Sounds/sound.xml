    <contact>
      <name>water contact</name>
      <mode>once</mode>
      <path>Aircraft/ec130/Sounds/BaBoOn-R44-Splash.wav</path>
      <condition>
        <not>
          <property>gear/gear[0]/ground-is-solid</property>
        </not>
        <and>
          <or>
            <property>gear/gear[8]/wow</property>
            <property>gear/gear[9]/wow</property>
            <property>gear/gear[10]/wow</property>
            <property>gear/gear[11]/wow</property>
            <less-than>
              <property>position/gear-agl-ft</property>
              <value>0.5</value>
            </less-than>
          </or>
