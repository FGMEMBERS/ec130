      <max-dist>1.0</max-dist>
    </warning>

    <!--actually there is no aural warning for overspeed!-->
    <warning>
      <type>avionics</type>
      <name>2600 Hz</name>
      <mode>looped</mode>
      <path>Aircraft/ec130/Sounds/warn2600.wav</path>
