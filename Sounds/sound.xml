<PropertyList>

  <fx>

    <avionic>
      <type>avionics</type>
      <name>avionic-cooling</name>
      <mode>looped</mode>
      <path>Aircraft/ec130/Sounds/avionics-400Hz.wav</path>
      <position>
        <x>-1.915</x>
        <y>-0.358245</y>
        <z>1.43194</z>
      </position>
      <reference-dist>1</reference-dist>
      <max-dist>2</max-dist>
      <volume>
        <property>/systems/electrical/outputs/avionics-switch</property>
        <max>0.15</max>
      </volume>
      <pitch>
        <property>/systems/electrical/outputs/avionics-switch</property>
        <factor>0.035</factor>
        <min>0.001</min>
        <max>1.0</max>
        <offset>0.0</offset>
      </pitch>
    </avionic>
  
    <HSI-gyro>
      <type>avionics</type>
      <name>HSIGyro</name>
      <mode>looped</mode>
      <path>Sounds/gear.wav</path>
      <position>
        <x>-1.915</x>
        <y>-0.358245</y>
        <z>1.43194</z>
      </position>
      <reference-dist>1</reference-dist>
      <max-dist>2</max-dist>
      <volume>
        <property>/instrumentation/kcs55/kg102/spin-norm</property>
        <max>0.1</max>
      </volume>
      <pitch>
        <property>/instrumentation/kcs55/kg102/spin-norm</property>
        <factor>0.75</factor>
        <min>0.001</min>
        <max>1.0</max>
        <offset>0.0</offset>
      </pitch>
    </HSI-gyro>

    <fuelpump>
      <type>avionics</type>
      <name>Fuelpump</name>
      <mode>looped</mode>
      <path>Sounds/fuelpump.wav</path>
      <position>
        <x>-1.915</x>
        <y>-0.358245</y>
        <z>1.43194</z>
      </position>
      <reference-dist>1</reference-dist>
      <max-dist>2</max-dist>
      <volume>
        <property>/systems/electrical/outputs/boost-pump</property>
        <max>0.1</max>
      </volume>
      <pitch>
        <property>/systems/electrical/outputs/boost-pump</property>
        <factor>0.035</factor>
        <min>0.001</min>
        <max>1.0</max>
        <offset>0.0</offset>
      </pitch>
    </fuelpump>

    <starter>
      <name>starter0_start</name>
      <mode>once</mode>
      <path>Aircraft/ec130/Sounds/starter_start.wav</path>
      <condition>
        <property>controls/engines/engine[0]/starter</property>
      </condition>
      <volume>
        <factor>0.1</factor>
      </volume>
      <position>
        <x>-0.88</x>
        <y>0.40</y>
        <z>0.5</z>
      </position>
      <reference-dist>5</reference-dist>
      <max-dist>10</max-dist>
    </starter>

    <starter>
      <name>starter0_loop</name>
      <mode>looped</mode>
      <path>Aircraft/ec130/Sounds/starter_loop.wav</path>
      <condition>
        <property>controls/engines/engine[0]/starter</property>
      </condition>
      <delay-sec>11.6</delay-sec>
      <volume>
        <factor>0.1</factor>
      </volume>
      <position>
        <x>-0.88</x>
        <y>0.40</y>
        <z>0.5</z>
      </position>
      <reference-dist>5</reference-dist>
      <max-dist>10</max-dist>
    </starter>

    <starter>
      <name>starter0_stop</name>
      <mode>once</mode>
      <path>Aircraft/ec130/Sounds/starter_stop.wav</path>
      <condition>
        <not>
          <property>controls/engines/engine[0]/starter</property>
        </not>
      </condition>
      <volume>
        <factor>0.3</factor>
      </volume>
      <position>
        <x>-0.88</x>
        <y>0.40</y>
        <z>0.5</z>
      </position>
      <reference-dist>5</reference-dist>
      <max-dist>10</max-dist>
    </starter>

    <engine>
      <name>turbine0-start</name>
      <mode>once</mode>
      <path>Aircraft/ec130/Sounds/strt_ec135_out2.wav</path>
      <condition>
      <greater-than>
        <property>engines/engine[0]/n1-pct</property>
        <value>0.1</value>
      </greater-than>
      </condition>
      <volume>
        <factor>0.03</factor>
      </volume>
      <volume>
        <property>sim/model/ec130/sound/volume</property>
        <factor>5</factor>
      </volume>
      <reference-dist>50</reference-dist>
      <max-dist>200</max-dist>
    </engine>

    <engine>
      <name>turbine0-loop</name>
      <mode>looped</mode>
      <path>Aircraft/ec130/Sounds/idle_ec135_out.wav</path>
      <condition>
        <property>engines/engine[0]/running</property>
      </condition>
      <delay-sec>45</delay-sec>
      <volume>
        <factor>0.03</factor>
      </volume>
      <volume>
        <property>sim/model/ec130/sound/volume</property>
        <factor>5</factor>
      </volume>
      <reference-dist>50</reference-dist>
      <max-dist>200</max-dist>
    </engine>

    <engine>
      <name>turbine0-shutdown</name>
      <mode>once</mode>
      <path>Sounds/turbine_shutdown.wav</path>
      <condition>
        <not>
          <property>engines/engine[0]/running</property>
        </not>
      </condition>
      <volume>
        <factor>0.03</factor>
      </volume>
      <volume>
        <property>sim/model/ec130/sound/volume</property>
        <factor>5</factor>
      </volume>
      <reference-dist>50</reference-dist>
      <max-dist>200</max-dist>
    </engine>

    <rotor>
      <name>rotor-outside</name>
      <mode>looped</mode>
      <path>Aircraft/ec130/Sounds/flight_ec135_out.wav</path>
      <condition>
        <and>
          <not>
            <property>sim/current-view/internal</property>
          </not>
          <greater-than>
            <property>rotors/main/rpm</property>
            <value>120</value>
          </greater-than>
          <not>
            <property>sim/crashed</property>
          </not>
        </and>
      </condition>
      <volume>
        <property>rotors/main/rpm</property>
        <factor>0.002</factor>
      </volume>
      <pitch>
        <property>rotors/main/rpm</property>
        <factor>0.00252</factor>
        <offset>0</offset>
        <min>0</min>
        <max>1.5</max>
      </pitch>
      <reference-dist>200</reference-dist>
      <max-dist>2000</max-dist>
    </rotor>

    <rotor>
      <name>rotor-inside</name>
      <mode>looped</mode>
      <path>Sounds/rotor.wav</path>
      <path>Aircraft/ec130/Sounds/ec_rotor_in.wav</path>
      <condition>
        <and>
          <property>sim/current-view/internal</property>
          <greater-than>
            <property>rotors/main/rpm</property>
            <value>120</value>
          </greater-than>
          <not>
            <property>sim/crashed</property>
          </not>
          <not>
            <property>sim/model/ec130/doors/frontl/position-norm</property>
          </not>
          <not>
            <property>sim/model/ec130/doors/frontr/position-norm</property>
          </not>
          <not>
            <property>sim/model/ec130/doors/passengerl/position-norm</property>
          </not>
          <not>
            <property>sim/model/ec130/doors/passengerr/position-norm</property>
          </not>
        </and>
      </condition>
      <volume>
        <factor>0.5</factor>
      </volume>
      <volume>
        <property>rotors/main/rpm</property>
        <factor>0.002</factor>
      </volume>
      <pitch>
        <property>rotors/main/rpm</property>
        <factor>0.00252</factor>
        <offset>0</offset>
        <min>0</min>
        <max>1.5</max>
      </pitch>
      <reference-dist>500</reference-dist>
      <max-dist>1000</max-dist>
    </rotor>

    <rotor>
      <name>rotor-inside-door-open</name>
      <mode>looped</mode>
      <path>Sounds/rotor.wav</path>
      <path>Aircraft/ec130/Sounds/flight_ec135_out.wav</path>
      <condition>
        <and>
          <property>sim/current-view/internal</property>
          <greater-than>
            <property>rotors/main/rpm</property>
            <value>120</value>
          </greater-than>
          <not>
            <property>sim/crashed</property>
          </not>
          <or>
            <property>sim/model/ec130/doors/frontl/position-norm</property>
            <property>sim/model/ec130/doors/frontr/position-norm</property>
            <property>sim/model/ec130/doors/passengerl/position-norm</property>
            <property>sim/model/ec130/doors/passengerr/position-norm</property>
          </or>
        </and>
      </condition>
      <volume>
        <factor>1</factor>
      </volume>
      <volume>
        <property>rotors/main/rpm</property>
        <factor>0.002</factor>
      </volume>
      <pitch>
        <property>rotors/main/rpm</property>
        <factor>0.00252</factor>
        <offset>0</offset>
        <min>0</min>
        <max>1.5</max>
      </pitch>
      <reference-dist>225</reference-dist>
      <max-dist>1000</max-dist>
    </rotor>

    <rotor>
      <name>rotor-inside-window-open</name>
      <mode>looped</mode>
      <path>Sounds/rotor.wav</path>
      <path>Aircraft/ec130/Sounds/flight_ec135_out.wav</path>
      <condition>
        <and>
          <property>sim/current-view/internal</property>
          <greater-than>
            <property>rotors/main/rpm</property>
            <value>120</value>
          </greater-than>
          <not>
            <property>sim/crashed</property>
          </not>
          <property>sim/model/ec130/doors/pilotw/position-norm</property>
        </and>
      </condition>
      <volume>
        <factor>0.4</factor>
      </volume>
      <volume>
        <property>rotors/main/rpm</property>
        <factor>0.002</factor>
      </volume>
      <pitch>
        <property>rotors/main/rpm</property>
        <factor>0.00252</factor>
        <offset>0</offset>
        <min>0</min>
        <max>1.5</max>
      </pitch>
      <reference-dist>225</reference-dist>
      <max-dist>1000</max-dist>
    </rotor>

    <rotor>
      <name>stall</name>
      <mode>looped</mode>
      <path>Aircraft/ec130/Sounds/R-1535Slow.wav</path>
      <condition>
        <and>
          <greater-than>
            <property>rotors/main/rpm</property>
            <value>50</value>
          </greater-than>
          <not>
            <property>sim/crashed</property>
          </not>
        </and>
      </condition>
      <volume>
        <property>sim/model/ec130/sound/volume</property>
        <factor>0.8</factor>
      </volume>
      <volume>
        <property>rotors/main/stall-filtered</property>
        <factor>75</factor>
        <max>1.0</max>
      </volume>
      <pitch>
        <property>rotors/main/rpm</property>
        <factor>0.00252</factor>
        <offset>0</offset>
        <max>1.75</max>
      </pitch>
      <reference-dist>500</reference-dist>
      <max-dist>1500</max-dist>
    </rotor>

    <rotor>
      <name>blade-vortex-interaction</name>
      <mode>looped</mode>
      <path>Aircraft/ec130/Sounds/R-1535Slow.wav</path>
      <condition>
        <greater-than>
          <property>sim/sound/vibration</property>
          <value>0.01</value>
        </greater-than>
      </condition>
      <volume>
        <property>sim/sound/vibration</property>
      </volume>
      <pitch>
        <property>rotors/main/rpm</property>
        <factor>0.00252</factor>
        <offset>0</offset>
        <min>0</min>
        <max>1.5</max>
      </pitch>
      <reference-dist>500</reference-dist>
      <max-dist>3000</max-dist>
    </rotor>

    <overspeed>
      <name>overspeed</name>
      <mode>looped</mode>
      <path>Sounds/overspeed.wav</path>
      <condition>
        <greater-than>
          <property>velocities/airspeed-kt</property>
          <value>146</value>
        </greater-than>
        <equals>
          <property>sim/current-view/view-number</property>
          <value>0</value>
        </equals>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
      <pitch>
        <factor>0.1</factor>
      </pitch>
      <reference-dist>1.0</reference-dist>
      <max-dist>2.0</max-dist>
    </overspeed>

    <warning>
      <type>avionics</type>
      <name>Cont310</name>
      <mode>looped</mode>
      <path>Aircraft/ec130/Sounds/Cont310.wav</path>
      <condition>
       <and>
        <property>sim/sound/Cont310</property>
        <greater-than>
          <property>/systems/electrical/outputs/horn</property>
          <value>24</value>
        </greater-than>
       </and>
       <not>
         <property>sim/crashed</property>
       </not>
      </condition>
      <volume>
        <factor>0.25</factor>
      </volume>
      <reference-dist>0.5</reference-dist>
      <max-dist>1.0</max-dist>
    </warning>
    
    <warning>
      <type>avionics</type>
      <name>Intermittent310</name>
      <mode>looped</mode>
      <path>Aircraft/ec130/Sounds/Intermittent310.wav</path>
      <condition>
       <and>
        <property>sim/sound/Intermittent310</property>
