<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>blade.ac</path>

  <animation>
    <object-name>blade</object-name>
  </animation>

  <animation>
    <type>select</type>
    <object-name>blade</object-name>
    <condition>
      <less-than>
        <property>rotors/tail/rpm</property>
	      <value>300</value>
      </less-than>
    </condition>
  </animation>

  <!-- blade effect -->
  <animation>
    <type>rotate</type>
    <object-name>blade</object-name>
    <property>rotors/tail/blade/incidence-deg</property>
    <factor>-1</factor>
    <center>
       <x-m> 0.000 </x-m>
       <y-m> 0.000 </y-m>
       <z-m> 0.000 </z-m>
    </center>
    <axis>
      <x> 1.000 </x>
      <y> 0.000 </y>
      <z> 0.000 </z>
    </axis>
  </animation>

</PropertyList>

