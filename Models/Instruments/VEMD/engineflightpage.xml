 <animation>
  <type>textranslate</type>
  <object-name>fuel</object-name>
  <property>consumables/fuel/tank/level-lbs</property>
  <interpolation>
   <entry><ind>  0</ind><dep>   0</dep></entry>
   <entry><ind>  16</ind><dep>0.053</dep></entry>
   <entry><ind> 262</ind><dep>0.125</dep></entry>
   <entry><ind>  524</ind><dep>0.25</dep></entry>
   <entry><ind>  1048</ind><dep>0.5</dep></entry>
       </interpolation>
