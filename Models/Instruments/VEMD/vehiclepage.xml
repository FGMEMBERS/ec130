<PropertyList>

  <path>vehiclepage.ac</path>

  <!--enginepage-->
  <animation>
    <type>select</type>
    <object-name>oilpressneedle</object-name>
    <object-name>oiltempneedle</object-name>
    <object-name>face</object-name>
    <condition>
      <greater-than>
        <property>instrumentation/VEMD/Phase/tested</property>
        <value>0</value>
      </greater-than>
      <not>
        <property>instrumentation/VEMD/Phase/shutdown</property>
        <value>1</value>
      </not>
    </condition>
  </animation>
 
  <animation>
    <type>rotate</type>
    <object-name>oilpressneedle</object-name>
    <property>/engines/engine/oil-pressure-bar</property>
    <interpolation>
      <entry><ind> 0.0</ind><dep>  0.00</dep></entry>
      <entry><ind> 1.0</ind><dep> 16.83</dep></entry>
      <entry><ind> 1.1</ind><dep> 19.94</dep></entry>
      <entry><ind> 2.0</ind><dep> 34.42</dep></entry>
      <entry><ind> 3.0</ind><dep> 53.10</dep></entry>
      <entry><ind> 4.0</ind><dep> 70.31</dep></entry>
      <entry><ind> 5.0</ind><dep> 86.81</dep></entry>
      <entry><ind> 6.0</ind><dep>103.12</dep></entry>
      <entry><ind> 7.0</ind><dep>121.12</dep></entry>
      <entry><ind> 8.0</ind><dep>138.70</dep></entry>
      <entry><ind> 9.0</ind><dep>157.16</dep></entry>
      <entry><ind> 9.8</ind><dep>170.47</dep></entry>
      <entry><ind>10.0</ind><dep>175.57</dep></entry>
    </interpolation>
    <center>
      <x-m>0.00</x-m>
      <y-m>0.079</y-m>
      <z-m>-0.021</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>oiltempneedle</object-name>
    <property>/engines/engine/oil-temperature-degc-filter</property>
    <interpolation>
      <entry><ind>-10</ind><dep>  0.00</dep></entry>
      <entry><ind>  0</ind><dep> 13.40</dep></entry>
      <entry><ind> 10</ind><dep> 26.55</dep></entry>
      <entry><ind> 20</ind><dep> 40.00</dep></entry>
      <entry><ind> 30</ind><dep> 52.00</dep></entry>
      <entry><ind> 40</ind><dep> 64.89</dep></entry>
      <entry><ind> 50</ind><dep> 77.68</dep></entry>
      <entry><ind> 60</ind><dep> 89.56</dep></entry>
      <entry><ind> 70</ind><dep>102.40</dep></entry>
      <entry><ind> 80</ind><dep>114.13</dep></entry>
      <entry><ind> 90</ind><dep>126.60</dep></entry>
      <entry><ind>100</ind><dep>139.25</dep></entry>
      <entry><ind>110</ind><dep>152.00</dep></entry>
      <entry><ind>115</ind><dep>157.97</dep></entry>
    </interpolation>
    <center>
      <x-m>0.00</x-m>
      <y-m>0.021</y-m>
      <z-m>-0.021</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <model>
    <name>text</name>
    <path>Aircraft/ec130/Models/Instruments/VEMD/vehiclepage-text.xml</path>
    <offsets>
      <x-m> 0.001</x-m>
      <y-m>0.023</y-m>
      <z-m>-0.028</z-m>
    </offsets>
    <overlay>
      <params>
        <property type="string">/engines/engine/oil-temperature-degc-filter</property>
        <factor type="double">1</factor>
        <offset type="double">0.0</offset>
        <format type="string">%3.0f</format>
        <color>
          <red   type="double">1.0</red>
          <green type="double">1.0</green>
          <blue  type="double">1.0</blue>
        </color>
      </params>
    </overlay>
  </model>

  <model>
    <name>text</name>
    <path>Aircraft/ec130/Models/Instruments/VEMD/vehiclepage-text.xml</path>
    <offsets>
      <x-m> 0.001</x-m>
      <y-m>0.081</y-m>
      <z-m>-0.028</z-m>
    </offsets>
    <overlay>
      <params>
        <property type="string">/engines/engine/oil-pressure-bar-filter</property>
        <factor type="double">1</factor>
        <offset type="double">0.0</offset>
        <format type="string">%3.0f</format>
        <color>
          <red   type="double">1.0</red>
          <green type="double">1.0</green>
          <blue  type="double">1.0</blue>
        </color>
      </params>
    </overlay>
  </model>
 
  <model>
    <name>text</name>
    <path>Aircraft/ec130/Models/Instruments/VEMD/vehiclepage-text.xml</path>
    <offsets>
      <x-m> 0.001</x-m>
      <y-m>0.032</y-m>
      <z-m>-0.051</z-m>
    </offsets>
    <overlay>
      <params>
        <property type="string">/systems/electrical/volts</property>
        <factor type="double">1</factor>
        <offset type="double">0.0</offset>
        <format type="string">%3.1f</format>
        <color>
          <red   type="double">1.0</red>
          <green type="double">1.0</green>
          <blue  type="double">1.0</blue>
        </color>
      </params>
    </overlay>
  </model>
 
  <model>
    <name>text</name>
    <path>Aircraft/ec130/Models/Instruments/VEMD/vehiclepage-text.xml</path>
    <offsets>
      <x-m> 0.001</x-m>
      <y-m>0.070</y-m>
      <z-m>-0.051</z-m>
    </offsets>
    <overlay>
      <params>
        <property type="string">/systems/electrical/amps</property>
        <factor type="double">1</factor>
        <offset type="double">0.0</offset>
        <format type="string">%3.1f</format>
        <color>
          <red   type="double">1.0</red>
          <green type="double">1.0</green>
          <blue  type="double">1.0</blue>
        </color>
      </params>
    </overlay>
  </model>

