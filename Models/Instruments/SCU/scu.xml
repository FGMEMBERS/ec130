 <path>scu2.ac</path>

  <animation>
    <type>pick</type>
    <object-name>batepu</object-name>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>controls/electric/battery-switch</property>
      </binding>
    </action>
  </animation>

  <animation>
    <type>select</type>
    <object-name>batepu_led</object-name>
