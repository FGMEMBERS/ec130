<!--emergency float -->
<PropertyList>
<path>left_float.ac</path>
<animation>
  <type>scale</type>
  <object-name>left_float</object-name>
    <property>sim/multiplay/generic/float[0]</property>
  <x-min>1.0</x-min>
  <y-min>1.0</y-min>
  <z-min>1.0</z-min>
  <x-factor>2</x-factor>
  <y-factor>10</y-factor>
  <z-factor>10</z-factor>
 </animation>
 <animation>
		<type>select</type>
		<object-name>left_float</object-name>
		<condition>
			<property>sim/model/ec130/emerg_floats</property>
		</condition>
</animation>
</PropertyList>