<!--light effect-->

  <animation>
    <type>light</type>
    <light-type>spot</light-type>
    <name>Landinglight</name>
    <object-name>LightVolume</object-name>
    <position>
      <x>-3.419</x>
      <y>-0.498</y>
      <z>-1.306</z>
    </position>
    <direction>
      <x>-1.0</x>
      <y> 0.0</y>
      <z> -0.35</z>
    </direction>
    <ambient>
      <r>0.00</r>
      <g>0.00</g>
      <b>0.00</b>
      <a>1.0</a>
    </ambient>
    <diffuse>
      <r>1.0</r>
      <g>1.0</g>
      <b>1.0</b>
      <a>1.0</a>
    </diffuse>
    <specular>
      <r>1.0</r>
      <g>1.0</g>
      <b>1.0</b>
      <a>1.0</a>
    </specular>
    <attenuation>
      <c>1.0</c>
	<l>0.225</l>
	<q>0.000100</q>
    </attenuation>
    <exponent>2</exponent>
    <near-m>0.001</near-m>
    <far-m>225</far-m>
    <cutoff>70</cutoff>
  </animation>

 
  
  
  <animation>
   <type>select</type>
  <object-name>LightVolume</object-name>   
  <nopreview/>
   <condition>
   <greater-than>
     <property>sim/multiplay/generic/int[1]</property>
     <value>24</value>
     </greater-than>
   </condition>
 </animation>


